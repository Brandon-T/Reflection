{$include_once srl/shared.simba}
{$include_once Timing.simba}

Function R_DidRedClick(): Boolean;
begin
  Result := WaitUntil((RGetInt(R_EIOS, nil, Client_CrossHairColor) = 2), 50, 200);
end;

Function R_DidYellowClick(): Boolean;
begin
  Result := WaitUntil((RGetInt(R_EIOS, nil, Client_CrossHairColor) = 1), 50, 200);
end;
